{
  "name": "petition-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --port 8080",
    "serve": "vue-cli-service serve --port 8080",
    "build": "vue-cli-service build --mode staging",
    "build-prod": "vue-cli-service build --mode prod",
    "kill-all": "fuser -k 8080/tcp 4941/tcp",
    "lint": "vue-cli-service lint",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "buefy": "^0.8.18",
    "core-js": "^3.6.4",
    "serve": "^11.3.0",
    "vue": "^2.6.11",
    "vue-axios": "^2.1.5",
    "vue-cli": "^2.9.6",
    "vue-fontawesome": "0.0.2",
    "vue-multiselect": "^2.1.6",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.1.6",
    "vuejs-logger": "^1.5.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/test-utils": "^1.0.0-beta.32",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "jest": "^25.1.0",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
